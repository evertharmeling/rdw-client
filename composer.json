{
  "name": "evertharmeling/rdw-client",
  "type": "library",
  "description": "Client library for the opendata.rdw.nl web service.",
  "keywords": [ "rdw", "api", "client", "open data" ],
  "homepage": "https://github.com/evertharmeling/rdw-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Evert Harmeling",
      "email": "evertharmeling@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "php-http/httplug": "^1.1",
    "guzzlehttp/psr7": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.3.5|^5.0",
    "php-http/mock-client": "^0.3.0"
  },
  "suggest": {
    "php-http/guzzle6-adapter": "An HTTPlug adapter for the Guzzle 6 HTTP client"
  },
  "autoload": {
    "psr-0": { "evertharmeling\\RDW": "lib/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "test": "./vendor/phpunit/phpunit/phpunit --coverage-text"
  }
}
